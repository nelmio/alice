{
    "name": "nelmio/alice",
    "description": "Expressive fixtures generator",
    "keywords": ["fixture", "data", "test", "orm"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jordi Boggiano",
            "email": "j.boggiano@seld.be"
        },
        {
            "name": "Tim Shelburne",
            "email": "shelburt02@gmail.com"
        },
        {
            "name": "Th√©o FIDRY",
            "email": "theo.fidry@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/theofidry/DeepCopy.git"
        }
    ],
    "require": {
        "php": "^7.0",
        "fzaninotto/faker": "^1.0",
        "symfony/yaml": "^2.0||^3.0",
        "myclabs/deep-copy": "dev-master"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.0",
        "symfony/property-access": "^2.2||^3.0"
    },
    "autoload": {
        "files": [
            "src/functions.php"
        ],
        "psr-4": {
            "Nelmio\\Alice\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nelmio\\Alice\\": [
                "tests",
                "fixtures"
            ]
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
